--Dance Monkey

local s = Instance.new("Sound")

s.Name = "Sound"
s.SoundId = "http://www.roblox.com/asset/?id=4911884241"
s.Volume = 100
s.Looped = false
s.archivable = false

s.Parent = game.Workspace

wait(3)

s:play()

local args = {
    [1] = "They say oh my God I see the way you shine",
